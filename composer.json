{
	"name" : "pgb-liv/phpMs-TopDown",
	"description" : "TopDown CLI utilities",
	"keywords" : [
		"massspec",
		"ms",
		"lcms",
		"msms"
	],
	"homepage" : "https://github.com/PGB-LIV/phpMs-TopDown",
	"license" : "Apache-2.0",
	"authors" : [{
			"name" : "Andrew Collins",
			"homepage" : "http://www.andrew-collins.co.uk"
		}
	],
	"require" : {
		"php" : ">=5.4.0",
		"pgb-liv/php-ms" : "dev-master",
		"symfony/console" : "2.8.*",
		"symfony/process" : "2.8.*",
		"symfony/symfony" : "2.8.*",
		"symfony/http-kernel" : "2.8.*",
		"symfony/monolog-bundle" : "2.8.*"
	},
	"require-dev" : {
		"theseer/phpdox" : "0.8.*",
		"squizlabs/php_codesniffer" : "3.*",
		"pdepend/pdepend" : "2.*",
		"sebastian/phpcpd" : "2.*",
		"phploc/phploc" : "2.*",
		"phpmd/phpmd" : "2.*",
		"phpunit/phpunit" : "4.*"
	},
	"autoload" : {
		"psr-4" : {
			"pgb_liv\\php_ms_example\\" : "src/"
		}
	},
	"support" : {
		"source" : "https://github.com/PGB-LIV/phpMs-TopDown"
	},
	"config" : {
		"vendor-dir" : "src/vendor"
	},
	"minimum-stability" : "stable"
}